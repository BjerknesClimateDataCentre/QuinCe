<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<description>Handles user login</description>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.User.LoginBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Processes user signup</description>
		<managed-bean-name>signupBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.User.SignupBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Email verification code checker</description>
		<managed-bean-name>verifyEmailBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.User.VerifyEmailBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Email verification code sender</description>
		<managed-bean-name>sendEmailCodeBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.User.SendEmailCodeBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>New Instrument</description>
		<managed-bean-name>newInstrumentBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.Instrument.NewInstrumentBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Small bean for producing lists of instruments</description>
		<managed-bean-name>instrumentListBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.Instrument.InstrumentListBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Handles viewing and editing of sensor calibrations</description>
		<managed-bean-name>calibrationsBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.Instrument.CalibrationsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Handles viewing and editing of gas standards</description>
		<managed-bean-name>standardsBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.Instrument.StandardsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Handles editing of the user profile</description>
		<managed-bean-name>userProfileBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.User.UserProfileBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Handles everything to do with the Jobs list page</description>
		<managed-bean-name>jobsBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.jobs.JobsBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Handles the File List page</description>
		<managed-bean-name>fileListBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.files.FileListBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Handles upload of data files</description>
		<managed-bean-name>uploadDataFileBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.files.UploadDataFileBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Holds all data required by the Data Screen, where the user performs QC.</description>
		<managed-bean-name>dataScreenBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.files.DataScreenBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>Bean that can be touched to keep user sessions alive</description>
		<managed-bean-name>keepAliveBean</managed-bean-name>
		<managed-bean-class>uk.ac.exeter.QuinCe.web.KeepAliveBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<validator>
		<description>Ensures that sensor calibration dates are unique</description>
		<display-name>Calibration Date Validator</display-name>
		<validator-id>calibrationDateValidator</validator-id>
		<validator-class>uk.ac.exeter.QuinCe.web.Instrument.CalibrationDateValidator</validator-class>
	</validator>
	<validator>
		<description>Ensures that gas standard dates are unique</description>
		<display-name>Gas Standard Date Validator</display-name>
		<validator-id>standardDateValidator</validator-id>
		<validator-class>uk.ac.exeter.QuinCe.web.Instrument.StandardDateValidator</validator-class>
	</validator>
	<validator>
		<description>Ensures that instrument names are not empty, and are unique to the user</description>
		<display-name>Instrument Name Validator</display-name>
		<validator-id>instrumentNameValidator</validator-id>
		<validator-class>uk.ac.exeter.QuinCe.web.Instrument.InstrumentNameValidator</validator-class>
	</validator>
	<navigation-rule>
		<display-name>all_pages</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>InternalError</from-outcome>
			<to-view-id>internalerror.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>SignUp</from-outcome>
			<to-view-id>/user/signup.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>AuthenticationFailed</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>AuthenticationSuccess</from-outcome>
			<to-view-id>/voyage/voyage_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SignupForm</display-name>
		<from-view-id>/user/signup.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ValidationFailed</from-outcome>
			<to-view-id>/user/signup.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>UserExists</from-outcome>
			<to-view-id>/user/signup.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/user/signup_complete.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SignupComplete</display-name>
		<from-view-id>/user/signup_complete.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Done</from-outcome>
			<to-view-id>/index.html</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>SendEmailCode</display-name>
		<from-view-id>/user/verify_email.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/user/code_sent.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ValidationFailed</from-outcome>
			<to-view-id>/index.html</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>InstrumentList</display-name>
		<from-view-id>/instrument/instrument_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>start</from-outcome>
			<to-view-id>/instrument/new/instrument_name.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>names</from-outcome>
			<to-view-id>/instrument/new/names.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>calibrations</from-outcome>
			<to-view-id>/instrument/calibrations.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>standards</from-outcome>
			<to-view-id>/instrument/standards.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>NewInstrumentNavigation</display-name>
		<from-view-id>/instrument/new/*</from-view-id>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/instrument/instrument_list.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>instrument_list</from-outcome>
			<to-view-id>/instrument/instrument_list.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>upload_files</from-outcome>
			<to-view-id>/instrument/new/upload_files.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CalibrationsList</display-name>
		<from-view-id>/instrument/calibrations.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>instrument_list</from-outcome>
			<to-view-id>/instrument/instrument_list.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>calibrations</from-outcome>
			<to-view-id>/instrument/calibrations.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>calibrationEditor</from-outcome>
			<to-view-id>/instrument/calibration_edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>StandardsList</display-name>
		<from-view-id>/instrument/standards.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>instrument_list</from-outcome>
			<to-view-id>/instrument/instrument_list.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>standards</from-outcome>
			<to-view-id>/instrument/standards.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>standardEditor</from-outcome>
			<to-view-id>/instrument/standard_edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>UserProfileEdit</display-name>
		<from-view-id>/user/user_edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>file_list</from-outcome>
			<to-view-id>/data/file_list.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>edit_profile</from-outcome>
			<to-view-id>/user/user_edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>FileList</display-name>
		<from-view-id>/data/file_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>upload_file</from-outcome>
			<to-view-id>/data/upload_file.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>file_list</from-outcome>
			<to-view-id>/data/file_list.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>data_screen</from-outcome>
			<to-view-id>/data/data_screen.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
			<from-outcome>export</from-outcome>
			<to-view-id>/data/export.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>UploadDataFile</display-name>
		<from-view-id>/data/upload_file.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>file_list</from-outcome>
			<to-view-id>/data/file_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>DataScreen</display-name>
		<from-view-id>/data/data_screen.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>file_list</from-outcome>
			<to-view-id>/data/file_list.xhtml</to-view-id>
		</navigation-case>
    </navigation-rule>
    <navigation-rule>
		<display-name>ExportFile</display-name>
		<from-view-id>/data/export.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>file_list</from-outcome>
			<to-view-id>/data/file_list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>


</faces-config>
